{
  "name": "web-env-init",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "start": "tauri dev",
    "build:prod": "tauri build",
    "prepare": "husky",
    "gitcz": "git-cz",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx,.vue --fix",
    "prettier": "prettier ./src/**/*.{js,jsx,ts,tsx,vue,css,less,sass,scss} --write"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.5.2",
    "plus-pro-components": "0.0.1-beta.37",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.5.0",
    "@commitlint/config-conventional": "^18.5.0",
    "@plus-pro-components/eslint-config": "^0.0.3",
    "@plus-pro-components/resolver": "^0.0.3",
    "@tauri-apps/cli": "^1.5.8",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "cz-git": "^1.8.0",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.20.1",
    "husky": "^9.0.1",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "sass": "^1.70.0",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.4",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,vue}": [
      "npm run lint"
    ],
    "src/**/*.{html,vue,css,sass,scss}": [
      "npm run stylelint"
    ]
  }
}
